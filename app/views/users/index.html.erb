<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
         Name: <%= user.name %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user) %>
        </span>
      </li>
      <%= friend_btn(user) %>
      <%= friend_accept(user) %>

    <% end %>

</ul>

</section>
